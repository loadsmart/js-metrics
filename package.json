  "description": "JavaScript Metrics by Loadsmart",
  "scripts": {
    "clean": "rm -rf .cache && rm -rf dist",
    "compile": "babel ./src/lib --out-dir ./dist --extensions '.js,.jsx' --ignore src/**/__tests__"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@datadog/browser-logs": "^1.26.3"
